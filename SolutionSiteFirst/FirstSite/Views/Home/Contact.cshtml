
@{
    ViewData["Title"] = "ارتباط با ما";
}

<button style="margin-bottom: 30px;margin-left: 50px;margin-top: 30px;">
    <a asp-action="Index" asp-controller="Home">صفحه اصلی</a>
</button>
<form action="post">
    <fieldset>
        <label for="email">ایمیل خود را وارد کنید </label>
        <input type="email" id="email" placeholder="for example Alirabieey@gmail.com" style="margin-left: 94px"><br><br>
        <label for="tel">شماره تماس :</label>
        <input type="tel" id="tel" placeholder="0921999999" style="margin-left: 141px;"><br><br>
        <label for="ramz">رمز عبور خود را وارد کنید:</label>
        <input type="password" id="ramz" placeholder="رمز خود را وارد کنید" style="margin-left: 61px;"> <br><br>
        <label for="pish">پیشنهادات و انتقادات خود را به ما بگویید </label>
        <input type="text" id="pish" placeholder=" نظر خود را بگویید" style="padding: 187px;"><br><br>
    </fieldset>
</form>


